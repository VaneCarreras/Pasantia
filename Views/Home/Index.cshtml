                productos.forEach(producto => {
                    container.innerHTML += `
                        <div class="col-lg-4 col-md-6 portfolio-item ${producto.CategoriaID} ${producto.MarcaID}">
                            <img src="${producto.ImagenesProducto}" class="img-fluid" alt="${producto.Descripcion}">
                            <div class="portfolio-info">
                                <h4>${producto.Descripcion}</h4>
                                
                                <a href="${producto.ImagenesProducto}" class="glightbox preview-link"><i class="bi bi-zoom-in"></i></a>
                            </div>
                        </div>
                    `;
                });
                initLightbox();
            }

            // Función para inicializar el Glightbox
            function initLightbox() {
                const lightbox = GLightbox();
            }

            // Filtro dinámico de productos
            const filters = document.querySelectorAll('.portfolio-filters li');
            filters.forEach(filter => {
                filter.addEventListener('click', function() {
                    const filterValue = this.getAttribute('data-filter');
                    const items = document.querySelectorAll('.portfolio-item');
                    
                    // Filtrar los productos
                    items.forEach(item => {
                        if (filterValue === '*' || item.classList.contains(filterValue)) {
                            item.style.display = 'block';
                        } else {
                            item.style.display = 'none';
                        }
                    });
                });
            });
        });
    </script> *@

